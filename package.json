{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "description": "nextjs boilerplate website with presets",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,json,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{js,json,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex289/NextJs-Boilerplate.git"
  },
  "keywords": [
    "Next.js",
    "NodeJs",
    "Vercel"
  ],
  "author": {
    "name": "Alexander Konietzko",
    "url": "https://alexanderkonietzko.vercel.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex289/NextJs-Boilerplate/issues"
  },
  "homepage": "https://next-js-boilerplate-sable.vercel.app",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.9",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.6.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.16.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.1",
    "@prisma/client": "^5.16.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "clsx": "^2.1.1",
    "geist": "^1.3.0",
    "lucide-react": "^0.399.0",
    "next": "14.2.4",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swr": "^2.2.5",
    "zod": "^3.23.8"
  },
  "nextBundleAnalysis": {
    "budget": 102400,
    "budgetPercentIncreaseRed": 20,
    "showDetails": true
  }
}
