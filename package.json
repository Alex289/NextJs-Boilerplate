{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "description": "nextjs boilerplate website with presets",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex289/NextJs-Boilerplate.git"
  },
  "keywords": [
    "Next.js",
    "NodeJs",
    "Vercel"
  ],
  "author": {
    "name": "Alexander Konietzko",
    "url": "https://alexanderkonietzko.vercel.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex289/NextJs-Boilerplate/issues"
  },
  "homepage": "https://next-js-boilerplate-sable.vercel.app",
  "devDependencies": {
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.61",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@prisma/client": "^5.10.2",
    "clsx": "^2.1.0",
    "geist": "^1.2.2",
    "lucide-react": "^0.343.0",
    "next": "14.1.0",
    "next-auth": "^4.24.6",
    "next-themes": "^0.2.1",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.5"
  },
  "nextBundleAnalysis": {
    "budget": 102400,
    "budgetPercentIncreaseRed": 20,
    "showDetails": true
  }
