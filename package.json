    "@tanstack/react-table": "^8.20.5",
    "@vercel/postgres": "^0.10.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "geist": "^1.3.1",
    "lucide-react": "^0.453.0",
    "next": "15.0.1",
    "next-auth": "5.0.0-beta.25",
    "next-safe-action": "^7.9.6",
    "next-themes": "^0.3.0",
    "nuqs": "^2.0.3",
