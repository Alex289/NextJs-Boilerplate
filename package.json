{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "description": "nextjs boilerplate website with presets",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex289/NextJs-Boilerplate.git"
  },
  "keywords": [
    "Next.js",
    "NodeJs",
    "Vercel"
  ],
  "author": {
    "name": "Alexander Konietzko",
    "url": "https://alexanderkonietzko.vercel.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex289/NextJs-Boilerplate/issues"
  },
  "homepage": "https://next-js-boilerplate-sable.vercel.app",
  "devDependencies": {
    "@types/eslint": "^8.44.8",
    "@types/node": "^20.10.3",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.42",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@prisma/client": "^5.6.0",
    "clsx": "^2.0.0",
    "geist": "^1.2.0",
    "lucide-react": "^0.294.0",
    "next": "14.0.3",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.4"
  },
  "nextBundleAnalysis": {
    "budget": 102400,
    "budgetPercentIncreaseRed": 20,
    "showDetails": true
  }
}
