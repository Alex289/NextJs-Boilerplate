{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "description": "nextjs boilerplate website with presets",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "NODE_ENV=production node server.js",
    "build": "next build",
    "lint": "eslint next.config.js server.js server/** components/** pages/**",
    "format": "prettier --write next.config.js server.js server/** pages/** components/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuzouSatoru2/NextJs-Boilerplate.git"
  },
  "keywords": [
    "NextJs",
    "Express",
    "Docker"
  ],
  "author": "Alex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JuzouSatoru2/NextJs-Boilerplate/issues"
  },
  "engines": {
    "npm": ">=6.13.7",
    "node": ">=10.14.2"
  },
  "homepage": "https://github.com/JuzouSatoru2/NextJs-Boilerplate#readme",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.6",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "next": "^9.5.3",
    "nprogress": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
