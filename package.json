{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "description": "nextjs boilerplate website with presets",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,json,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{js,json,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex289/NextJs-Boilerplate.git"
  },
  "keywords": [
    "Next.js",
    "NodeJs",
    "Vercel"
  ],
  "author": {
    "name": "Alexander Konietzko",
    "url": "https://alexanderkonietzko.vercel.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex289/NextJs-Boilerplate/issues"
  },
  "homepage": "https://alex-boilerplate.vercel.app",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.9",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.26.2",
    "eslint": "^9.13.0",
    "eslint-config-next": "15.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "pg": "^8.13.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.2",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
