{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "description": "nextjs boilerplate website with presets",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex289/NextJs-Boilerplate.git"
  },
  "keywords": [
    "Next.js",
    "NodeJs",
    "Vercel"
  ],
  "author": {
    "name": "Alexander Konietzko",
    "url": "https://alexanderkonietzko.vercel.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex289/NextJs-Boilerplate/issues"
  },
  "homepage": "https://next-js-boilerplate-sable.vercel.app",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.33",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "prisma": "^4.12.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@next/font": "^13.3.0",
    "@prisma/client": "^4.12.0",
    "@vercel/og": "^0.5.0",
    "lucide-react": "^0.130.1",
    "next": "13.3.0",
    "next-auth": "^4.21.1",
    "next-themes": "^0.2.1",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.1.2"
  },
  "nextBundleAnalysis": {
    "budget": 102400,
    "budgetPercentIncreaseRed": 20,
    "showDetails": true
  }
